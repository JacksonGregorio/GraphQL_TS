{
  "name": "my-sequelize-app",
  "version": "1.0.0",
  "description": "A Node.js application using Sequelize with TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",

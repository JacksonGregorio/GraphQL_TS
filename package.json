  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.23",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^14.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.4",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.3",
